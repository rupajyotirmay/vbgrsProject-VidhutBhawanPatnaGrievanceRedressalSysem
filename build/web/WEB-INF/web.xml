    <servlet>
        <servlet-name>MailSend</servlet-name>
        <servlet-class>MailSend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailServletTest</servlet-name>
        <servlet-class>EmailServletTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MailApp</servlet-name>
        <servlet-class>MailApp.MailApp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>empupdt</servlet-name>
        <servlet-class>empupdt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>comupdt</servlet-name>
        <servlet-class>comupdt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addemp</servlet-name>
        <url-pattern>/addemp</url-pattern>
    </servlet-mapping>
