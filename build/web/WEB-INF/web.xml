        <servlet-name>empupdt</servlet-name>
        <url-pattern>/empupdt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>comupdt</servlet-name>
        <url-pattern>/comupdt</url-pattern>
    </servlet-mapping>
