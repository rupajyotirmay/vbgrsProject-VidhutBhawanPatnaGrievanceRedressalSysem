        <servlet-name>addconsumer</servlet-name>
        <url-pattern>/addconsumer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewprofile</servlet-name>
        <url-pattern>/viewprofile</url-pattern>
    </servlet-mapping>
