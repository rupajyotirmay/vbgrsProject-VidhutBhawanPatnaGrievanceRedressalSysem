        <servlet-name>MailSend</servlet-name>
        <url-pattern>/MailSend</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailServletTest</servlet-name>
        <url-pattern>/EmailServletTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MailApp</servlet-name>
        <url-pattern>/MailApp</url-pattern>
    </servlet-mapping>
